  return {\n        ...state,\n        actor_series: {\n          ...state.actor_series,\n          [action.id]: {\n            cast: action.cast,\n            crew: action.crew,\n          },\n        },\n        updated: true,\n      };\n\n    case types.GET_USER:\n      return {\n        ...state,\n        users: {\n          ...state.users,\n          [action.id]: action.user,\n        },\n        updated: true,\n      };\n\n    case types.ALL_USERS:\n      return {\n        ...state,\n        users: action.users,\n        updated: true,\n      };\n\n    default:\n      return state;\n  }\n}\n","/Users/ashleydyson/Documents/Petio/petio/admin/src/page/settings/radarr.js",["208","209","210","211","212","213"],"/Users/ashleydyson/Documents/Petio/petio/admin/src/page/settings/general.js",["214"],"/Users/ashleydyson/Documents/Petio/petio/admin/src/page/settings/sonarr.js",["215","216","217","218","219","220"],"/Users/ashleydyson/Documents/Petio/petio/admin/src/data/Api/actions.js",["221","222"],"/Users/ashleydyson/Documents/Petio/petio/admin/src/data/User/index.js",["223"],"import { login, logout, getRequests } from './actions';\n\nexport default {\n\tlogin,\n\tlogout,\n\tgetRequests,\n};\n","/Users/ashleydyson/Documents/Petio/petio/admin/src/data/Plex/index.js",["224"],"import { plexAuth } from './actions';\n\nexport default {\n\tplexAuth,\n};\n","/Users/ashleydyson/Documents/Petio/petio/admin/src/components/SessionMedia.js",["225","226","227","228","229"],"import React from 'react';\nimport { withRouter, Link } from 'react-router-dom';\nimport { connect } from 'react-redux';\nimport Api from '../data/Api';\nimport { LazyLoadImage } from 'react-lazy-load-image-component';\nimport 'react-lazy-load-image-component/src/effects/blur.css';\nimport MovieCard from './MovieCard';\nimport TvCard from './TvCard';\nimport { ReactComponent as PlayIcon } from '../assets/svg/play.svg';\nimport { ReactComponent as PauseIcon } from '../assets/svg/pause.svg';\nimport { ReactComponen